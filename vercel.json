{
       "version": 2,
       "builds": [
         {
           "src": "api/**/*.js",
           "use": "@vercel/node"
         },
         {
           "src": "index.html",
           "use": "@vercel/static"
         },
         {
           "src": "src/main.js",
           "use": "@vercel/vue"
         }
       ],
       "routes": [
         {
           "src": "/api/(.*)",
           "dest": "/api/$1"
         },
         {
           "src": "/(.*)",
           "dest": "/dist/$1"
         }
       ]
     }